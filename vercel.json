{
    "version": 2,
    "builds": [
      {
        "src": "public/js/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/css/*.css",
        "use": "@vercel/static"
      },
      {
        "src": "public/html/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/js/(.*)", "dest": "public/js/$1" },
      { "src": "/css/(.*)", "dest": "public/css/$1" },
      { "src": "/html/(.*)", "dest": "public/html/$1" },
      { "src": "/", "dest": "/public/html/ethereum.html" }
    ]
  }
  